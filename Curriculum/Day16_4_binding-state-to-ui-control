import SwiftUI

struct ContentView: View {
    @State private var storedText = String()
    
    var body: some View {
        Form {
            TextField("Enter your name here", text: $storedText)
            Text("Hello, \(storedText)!")
        }
        
    }
}

struct ContentView_Previews: PreviewProvider {
    static var previews: some View {
        ContentView()
    }
}
